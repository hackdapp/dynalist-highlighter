{
  "name": "Dynalist Highlighter",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "./icon/dynalist-highlighter-logo-16.png",
    "48": "./icon/dynalist-highlighter-logo-48.png",
    "128": "./icon/dynalist-highlighter-logo-128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {},
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": ["storage", "activeTab", "http://*/", "https://*/"]
}
