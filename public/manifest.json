{
  "name": "Dynalist Highlighter",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "./icon/icon-on-16.png",
    "48": "./icon/icon-on-48.png",
    "128": "./icon/icon-on-128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {},
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": ["storage", "activeTab", "<all_urls>", "contextMenus"]
}
